(MyRepository new instVarNamed: 'name' put: 'ConferenceSystemDP';addComponents: {'ReviewerNet' -> (PNCompiledClass compileClass: 'class ReviewerNet is_a PNobject	place conference()	place member()	place start(1`#e)	trans test		precond start(1`#e)		action {			Transcript show: ''Reviewer net'' . Transcript cr .  }	sync member: m 		cond member(1`m)	sync cannotPutReview		cond conference(1`c)		guard { c phase: #final .  }	sync canPutReview		cond conference(1`c)		guard { c phase: #review .  }method for: m 	place m()	place return()	trans t		precond m(1`member)		action {			self setMember: member .  }		postcond return(1`#done)method setConference: c 	place c()	place return()	trans t		precond c(1`c)		postcond conference(1`c), return(1`c)method setMember: m 	place m()	place return()	trans t		precond m(1`m)		postcond member(1`m), return(1`m)method addReview: text to: paper 	place text()	place paper()	place return()	trans succes		cond member(1`m)		precond text(1`t), paper(1`p)		guard { self canPutReview .  }		action {			rev = (Review new ) . rev setAuthor: m . rev setText: text . p putReview: rev .  }		postcond return(1`true)	trans fail		cond conference(1`c), member(1`m)		precond r(1`p)		guard { self cannotPutReview .  }		postcond return(1`false)method for: m in: c 	place m()	place c()	place return()	trans t		precond m(1`member), c(1`conf)		action {			self setMember: member . self setConference: conf .  }		postcond return(1`#done)').'Conference' -> (PNCompiledClass compileClass: 'class Conference is_a PNobject	place name(1`#nil)	place description(1`#nil)	place datetime(1`#nil)	place location(1`#nil)	place papers()	place papersCount(1`0)	place papersTmp()	place papersLock(1`#free)	place members()	place membersCount(1`0)	place membersTmp()	place membersLock(1`#free)	place state(1`#open)	sync reviewer: r 		cond members(1`r)		guard { r isReviewer .  }	sync name: n 		cond name(1`n)	inhibitor isPapersTmp: obj 		cond papersTmp(1`obj)	sync papersCount: c 		cond papersCount(1`c)	sync author: a 		cond members(1`a)		guard { a isAuthor .  }	sync member: m named: n 		cond members(1`m)		guard { m name: n .  }	inhibitor isMembersTmp: obj 		cond membersTmp(1`obj)	inhibitor descNotEmpty		cond description(1`d)		guard { d == #nil .  }	sync membersCount: c 		cond membersCount(1`c)	sync state: s 		cond state(1`s)	sync nameEmpty		cond name(1`n)		guard { n != #nil .  }	inhibitor notMemberNamed: n 		cond members(1`m)		guard { m name: n .  }	inhibitor membersEmpty		cond members(1`obj)	sync description: d 		cond description(1`d)	inhibitor papersEmpty		cond papers(1`obj)method addPaper: p 	place p()	place return()	trans t		precond p(1`p), papersCount(1`x)		action {			y := x + 1 .  }		postcond papers(1`p), papersCount(1`y), return(1`p)method setName: newName 	place newName()	place return()	trans t		precond newName(1`n), name(1`nOld)		postcond name(1`n), return(1`n)method printPapers	place start(1`#e)	place phase()	place count()	place return()	trans empty		precond phase(1`#read), count(1`0)		guard { self papersEmpty .  }		action {			Transcript show: ''Papers empty'' . Transcript cr .  }		postcond return(1`#nil)	trans read		cond papers(1`p), phase(1`#read)		precond count(1`c)		guard { self isPapersTmp: p .  }		action {			newCount := c + 1 . Transcript show: (newCount printString ) . Transcript show: ''- '' . p print . p printAuthors . p printReview .  }		postcond papersTmp(1`p), count(1`newCount)	trans readDone		cond count(1`myCount), papersCount(1`pCount)		precond phase(1`#read)		guard { myCount = pCount .  }		postcond phase(1`#done)	trans clearTmp		cond phase(1`#done)		precond papersTmp(1`p), count(1`count)		guard { count > 0 .  }		action {			newCount := count - 1 .  }		postcond count(1`newCount)	trans foreachFinish		precond phase(1`#done), count(1`0)		postcond 